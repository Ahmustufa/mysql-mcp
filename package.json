{
  "name": "@ahmedmustafamalik/mysql-mcp",
  "version": "1.2.3",
  "description": "MCP server for MySQL database interactions",
  "main": "build/index.js",
  "bin": {
    "@ahmedmustafamalik/mysql-mcp": "./build/index.js",
    "mysql-mcp": "build/index.js"
  },
  "repository": {
    "url": "https://github.com/Ahmustufa/mysql-mcp",
    "type": "git"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsx watch src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf build",
    "docker:build": "docker build -t mysql-mcp .",
    "docker:run": "docker run --env-file .env mysql-mcp",
    "docker:compose": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f mysql-mcp",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:connection": "node -e \"console.log('Connection test not implemented yet')\"",
    "test:integration": "echo \"Integration tests not implemented yet\"",
    "test:load": "echo \"Load tests not implemented yet\""
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mcp",
    "mysql",
    "database",
    "llm"
  ],
  "author": "Ahmed Mustufa Malik",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "dotenv": "^17.2.0",
    "mysql2": "^3.15.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
